
<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('partials/meta',{ time:time}) %>
  <title><%= title %></title>
  <!-- Font Awesome -->
  <link rel="stylesheet" href="/cdn/fontawesome/css/all.min.css">
  <% if(!IS_PRODUCTION) { %>
  <link rel="stylesheet" href="/css/utils.css?v=<%= time %>">
  <link rel="stylesheet" href="/css/fontsAnuphan.css?v=<%= time %>">
  <link rel="stylesheet" href="/css/alink.css?v=<%= time %>">
  <link rel="stylesheet" href="/css/toast.css?v=<%= time %>">
  <link rel="stylesheet" href="/css/cb.css?v=<%= time %>">
  <link rel="stylesheet" href="/css/input.css?v=<%= time %>">
  <link rel="stylesheet" href="/css/nav.css?v=<%= time %>">
  <link rel="stylesheet" href="/css/same.css?v=<%= time %>">
  <link rel="stylesheet" href="/css/manageSettings.css?v=<%= time %>">
  <% } else { %>
  <link rel="stylesheet" href="/css-min/manageSettings.min.css?v=<%= time %>">
  <% }  %>
</head>
<body>

  <%- include('partials/notify.ejs', {msg:msg}) %> 
  <%- include('partials/nav.ejs') %>
  
  <main>

    <section class="pd-b-s">
      <div class="pd-l">
        <h1 class="al-c">
          <i class="fas fa-sliders-h mg-r-m"></i> 
          <span><%= title %></span>
        </h1>
      </div>
    </section>

    <section>
      
      <!-- Tab links -->
      <div class="tab">
        <div class="tabMenu col-20" id="tab1Menu" onclick="event.preventDefault();openMenu(this,'tabcontent1')">ทั่วไป</div>
        <div class="tabMenu col-20" id="tab2Menu" onclick="event.preventDefault();openMenu(this,'tabcontent2')"><%= PAGE_QUOTATION %></div>
        <div class="tabMenu col-20" id="tab3Menu" onclick="event.preventDefault();openMenu(this,'tabcontent3')"><%= PAGE_INVOICE %></div>
        <div class="tabMenu col-20" id="tab4Menu" onclick="event.preventDefault();openMenu(this,'tabcontent4')"><%= PAGE_RECEIPT %></div>
        <div class="tabMenu col-20" id="tab5Menu" onclick="event.preventDefault();openMenu(this,'tabcontent5')"><%= PAGE_BILL %></div>
      </div>

      <!-- #1 -->
      <div id="tabcontent1" class="tabcontent">
        <%- include('partials/manageSettings_general.ejs' ) %>
      </div>

      <!-- #2 -->
      <div id="tabcontent2" class="tabcontent">
        <%- include('partials/manageSettings_quotation.ejs' ) %>
      </div>

      <!-- #3 -->
      <div id="tabcontent3" class="tabcontent">
        <%- include('partials/manageSettings_invoice.ejs' ) %>
      </div>

      <!-- #4 -->
      <div id="tabcontent4" class="tabcontent">
        <%- include('partials/manageSettings_receipt.ejs' ) %>
      </div>
      
      <!-- #5 -->
      <div id="tabcontent5" class="tabcontent">
        <%- include('partials/manageSettings_bill.ejs' ) %>
      </div>

    </section>

    <!--  -->

  </main>

  <!--  -->
  <%- include('partials/footer') %>

  <script type="text/javascript">
    const tabNumberClicked = `<%= tabNumberClicked %>`;
  </script>
  <!--  -->
  <% if(!IS_PRODUCTION) { %>
  <script type="text/javascript" src="/js/script_all.js?v=<%= time %>"></script>
  <script type="text/javascript" src="/js/nav.js"></script>
  <script type="text/javascript" src="/js/notify.js?v=<%= time %>"></script>
  <script type="text/javascript" src="/js/manageSettings.js?v=<%= time %>"></script>
  <% } else { %>
  <script type="text/javascript" src="/js-min/manageSettings.obf.js?v=<%= time %>"></script>
  <% }  %>
</body>
</html>




